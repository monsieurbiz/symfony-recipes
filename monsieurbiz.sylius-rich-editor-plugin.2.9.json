                    "contents": [
                        "monsieurbiz_richeditor_admin:",
                        "    resource: \"@MonsieurBizSyliusRichEditorPlugin/Resources/config/routing/admin.yaml\"",
                        "    prefix: /%sylius_admin.path_name%",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "982c5e821504e00c6a50dfef7e59b6a4bc3377b5"
        }
    }
