{
    "manifests": {
        "monsieurbiz/sylius-anti-spam-plugin": {
            "manifest": {
                "bundles": {
                    "MonsieurBiz\\SyliusAntiSpamPlugin\\MonsieurBizSyliusAntiSpamPlugin": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "RECAPTCHA3_SCORE_THRESHOLD": 0.5,
                    "RECAPTCHA3_ENABLED": "false",
                    "ALTCHA_ENABLED": "true"
                },
                "post-install-output": [
                    "  * Override existing config files for recaptcha and altcha to use env vars",
                    "    <comment>cp -r vendor/monsieurbiz/sylius-anti-spam-plugin/dist/config config/</comment>"
                ]
            },
            "files": {
                "config/packages/monsieurbiz_sylius_anti_spam_plugin.yaml": {
                    "contents": [
                        "imports:",
                        "    - { resource: \"@MonsieurBizSyliusAntiSpamPlugin/Resources/config/config.yaml\" }",
                        "",
                        "services:",
                        "    # Add the \"monsieurbiz_anti_spam.quarantineable\" tag on the quarantineable entity (not autoconfigure the entity\u2026)",
                        "    App\\Entity\\Customer\\Customer:",
                        "        tags: ['monsieurbiz_anti_spam.quarantineable']",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/monsieurbiz_sylius_anti_spam_plugin.yaml": {
                    "contents": [
                        "monsieurbiz_sylius_anti_spam_admin:",
                        "    resource: \"@MonsieurBizSyliusAntiSpamPlugin/Resources/config/routes/admin.yaml\"",
                        "    prefix: /%sylius_admin.path_name%",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "11631e8e8613db12fafd14b466e10b608b78a330"
        }
    }
}
