{
    "manifests": {
        "monsieurbiz/sylius-cms-page-plugin": {
            "manifest": {
                "bundles": {
                    "MonsieurBiz\\SyliusCmsPagePlugin\\MonsieurBizSyliusCmsPagePlugin": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                }
            },
            "files": {
                "config/packages/doctrine_override.yaml": {
                    "contents": [
                        "doctrine:",
                        "    orm:",
                        "        validate_xml_mapping: false # See https://github.com/Sylius/Sylius/issues/18304",
                        ""
                    ],
                    "executable": false
                },
                "config/packages/monsieurbiz_sylius_cms_page_plugin.yaml": {
                    "contents": [
                        "imports:",
                        "    - { resource: \"@MonsieurBizSyliusCmsPagePlugin/Resources/config/config.yaml\" }",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/monsieurbiz_sylius_cms_page_plugin.yaml": {
                    "contents": [
                        "monsieurbiz_cms_page_admin:",
                        "    resource: \"@MonsieurBizSyliusCmsPagePlugin/Resources/config/routing/admin.yaml\"",
                        "    prefix: /%sylius_admin.path_name%",
                        "",
                        "# Show page",
                        "monsieurbiz_cms_page_show:",
                        "    path: /{_locale}/{slug}",
                        "    methods: [GET]",
                        "    requirements:",
                        "        slug: .+",
                        "        _locale: ^[A-Za-z]{2,4}(_([A-Za-z]{4}|[0-9]{3}))?(_([A-Za-z]{2}|[0-9]{3}))?$",
                        "    defaults:",
                        "        _controller: monsieurbiz_cms_page.controller.page::showAction",
                        "        _sylius:",
                        "            template: \"@MonsieurBizSyliusCmsPagePlugin/shop/page/show.html.twig\"",
                        "            repository:",
                        "                method: findOneEnabledAndPublishedBySlugAndChannelCode",
                        "                arguments:",
                        "                    - $slug",
                        "                    - \"expr:service('sylius.context.locale').getLocaleCode()\"",
                        "                    - \"expr:service('sylius.context.channel').getChannel().getCode()\"",
                        "                    - \"expr:service('monsieurbiz.cms_page.datetime_provider').now()\"",
                        "    condition: \"not(context.getPathInfo() matches '`^%sylius.security.api_route%`') and context.checkPageSlug(request)\"",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "eda711f94381c4493909343042c43f9e9ca24cc5"
        }
    }
}
